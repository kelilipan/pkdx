{
  "rewrites": [
    {
      "source": "/pokemon/sprites/:match*",
      "destination": "https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/:match*",
      "has": [
        {
          "type": "header",
          "key": "cache-control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    },
    {
      "source": "/pokemon/animated/:match*",
      "destination": "https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/versions/generation-v/black-white/animated/:match*",
      "has": [
        {
          "type": "header",
          "key": "cache-control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    }
  ]
}
